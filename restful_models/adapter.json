                "tags": [
                    "system"
                ],
                "summary": "Heart Beat",
                "description": "ENMS heart beat, to check the system health status",
                "produces": [
                    "application/json"
                ],
                "operationId": "checkHeartbeat",
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                }
            }
        }
    },
    "definitions": {
        "TP": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "NE001"
                },
                "aid": {
                    "type": "string",
                    "example": 10000
                },
                "neId": {
                    "type": "string",
                    "example": 10000
                },
                "userLabel": {
                    "type": "string",
                    "example": "NE001/Port1"
                },
                "nativeName": {
                    "type": "string"
                },
                "layerRate": {
                    "type": "string",
                    "example": "SDH155M"
                },
                "tpType": {
                    "type": "string",
                    "example": "etc/sdh/wdm/stm16etc."
                },
                "usedByConnection": {
                    "type": "string",
                    "description": "Connection id that using the TP\n-1 - TP not used; \n0 - TP occupied by XC, but this XC not belongs to connection\n>0 - real connection ID\n"
                },
                "tpgMembers": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "comment": {
                    "type": "string"
                }
            }
        },
        "CONNECTION": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string",
                    "example": "c12345"
                },
                "userLabel": {
                    "type": "string"
                },
                "nativeName": {
                    "type": "string"
                },
                "layerRate": {
                    "type": "string",
                    "example": "STM16"
                },
                "atps": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/TP"
                    }
                },
                "ztps": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/TP"
                    }
                },
                "connectionType": {
                    "type": "string",
                    "example": "P2P/Tree/Lan/Ring/group",
                    "description": "the type of the connection, 'group' means it the connection group"
                },
                "groupMembers": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/CONNECTION"
                    }
                },
                "serverConnections": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/CONNECTION"
                    }
                },
                "xcs": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/XC"
                    }
                },
                "direction": {
                    "type": "string",
                    "example": "bidirectional/unidirectional"
                },
                "definitionTime": {
                    "type": "string",
                    "example": "2016-07-11T12:01:02.000Z"
                }
            }
        },
        "User": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "username": {
                    "type": "string"
                },
                "firstName": {
                    "type": "string"
                },
                "lastName": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "phone": {
                    "type": "string"
                },
                "userStatus": {
                    "type": "integer",
                    "format": "int32",
                    "description": "User Status"
                }
            }
        },
        "NE": {
            "type": "object",
            "required": [
                "userLabel",
                "neType"
            ],
            "properties": {
                "id": {
                    "type": "string",
                    "example": 12345
                },
                "aid": {
                    "type": "string",
                    "example": 10000
                },
                "userLabel": {
                    "type": "string",
                    "example": "NE01/01"
                },
                "nativeName": {
                    "type": "string"
                },
                "neType": {
                    "type": "string",
                    "example": "1662mc"
                },
                "techDomain": {
                    "type": "string",
                    "description": "the domain of NE technologies",
                    "example": "SDH|MSTP|PTN|OTN|IP|…"
                },
                "version": {
                    "type": "string",
                    "example": 6
                },
                "address": {
                    "type": "string",
                    "example": 4.700058e+23
                },
                "addressType": {
                    "type": "string",
                    "example": "Q3|IPv4Telnet|IPv4SNMPv2|IPv4SNMPv3…"
                },
                "supervisionStatus": {
                    "type": "string",
                    "example": "declared/supervised"
                },
                "operationStatus": {
                    "type": "string",
                    "example": "enabled/disabled"
                },
                "communicationStatus": {
                    "type": "string",
                    "example": "reachable/unreachable"
                },
                "alignmentStatus": {
                    "type": "string",
                    "example": "aligned/misaligned"
                },
                "loginname": {
                    "type": "string",
                    "example": "admin"
                },
                "password": {
                    "type": "string",
                    "example": "pwd"
                },
                "coordinates": {
                    "type": "object",
                    "description": "The GPS coordinates of the NE",
                    "properties": {
                        "longitude": {
                            "type": "string"
                        },
                        "latitude": {
                            "type": "string"
                        }
                    }
                },
                "extraInfo": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "key": {
                                "type": "string"
                            },
                            "value": {
                                "type": "string"
                            }
                        }
                    },
                    "description": "A list to store key/value extra info. test 3"
                },
                "comment1": {
                    "type": "string"
                },
                "comment2": {
                    "type": "string"
                }
            }
        },
        "EQUIPMENT": {
            "type": "object",
            "required": [
                "neId"
            ],
            "properties": {
                "aid": {
                    "type": "string",
                    "example": "NE001/Board1"
                },
                "neId": {
                    "type": "string"
                },
                "userLabel": {
                    "type": "string"
                },
                "nativeName": {
                    "type": "string"
                }
            }
        },
        "XC": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "string"
                },
                "aid": {
                    "type": "string"
                },
                "ne": {
                    "$ref": "#/definitions/NE"
                },
                "layerrate": {
                    "type": "string"
                },
                "atps": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/TP"
                    }
                },
                "ztps": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/TP"
                    }
                },
                "usedByConnection": {
                    "type": "string",
                    "description": "Connection id that using the TP\n-1 - XC not used, isolated XC, need to be cleaned by NMS \n0 - not valid, don't use this value\n>0 - real connection ID\n"
                },
                "implStatus": {
                    "type": "string"
                }
            }
        },
        "ROUTE": {
            "type": "object",
            "properties": {
                "sequence": {
                    "type": "integer"
                },
                "xcs": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/XC"
                    }
                }
            }
        },
        "NESTATISTICS": {
            "type": "object",
            "properties": {
                "neType": {
                    "type": "string"
                },




                "protocols": {
                    "type": "array",
                    "description": "the list of supported protocols",
                    "items": {
                        "type": "string"
                    }
                },
                "versions": {
                    "type": "array",
                    "description": "the list of available NE versions",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "NEASSIGNMENT": {
            "type": "object",
            "description": "The NE assignment information, used for NE assignment and NE status report",
            "properties": {
                "adapterID": {
                    "type": "string",
                    "description": "the adapter ID, optional"
                },
                "assignTime": {
                    "type": "string"
                },
                "status": {
                    "type": "string",
                    "description": "If the NE currently active on the adapter, active or inactive",
                    "example": "active|inactive|disconnected"
                },
                "ne": {
                    "$ref": "#/definitions/NE"
                },
                "remark": {
                    "type": "string",
                    "description": "the extra info"
                }
            }
        }
    }
}
